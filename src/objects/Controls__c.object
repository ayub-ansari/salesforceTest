    <fields>
        <fullName>Next_Assessment_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Assessment_Frequency__c, &quot;Daily&quot;), Last_Assessed__c+1, 

IF(ISPICKVAL(Assessment_Frequency__c, &quot;Weekly&quot;), Last_Assessed__c+7, 

IF(ISPICKVAL(Assessment_Frequency__c, &quot;Monthly&quot;), Last_Assessed__c+ CASE(MONTH(Last_Assessed__c+1),1, 
IF(MOD(Year(Last_Assessed__c),4)=0,CASE(DAY(Last_Assessed__c),29,31,30,30,31,29,31),CASE(DAY(Last_Assessed__c),30,29,31,28,31)), 
2,IF(MOD(Year(Last_Assessed__c),4)=0,29,28),3,31,4,30,5,31,6,30,7,31,8,31,9,30,10,31,11,30,12,31,NULL), 

IF(ISPICKVAL(Assessment_Frequency__c, &quot;Quarterly&quot;), Last_Assessed__c+CASE(MONTH(Last_Assessed__c+1), 1,IF(MOD(Year(Last_Assessed__c+1),4)=0,91,90),2,IF(MOD(Year(Last_Assessed__c+1),4)=0,90,89),3,92,4,91,5,92,6,92,7,92,8,92,9,91,10,92,11, 
IF(MOD(Year(Last_Assessed__c)+1,4)=0,CASE(DAY(Last_Assessed__c),29,92,30,91,92),CASE(DAY(Last_Assessed__c),30,90,92)), 
12,IF(MOD(Year(Last_Assessed__c+1)+1,4)=0,91,90),NULL), 

IF(ISPICKVAL(Assessment_Frequency__c, &quot;Semiannually&quot;), Last_Assessed__c+CASE(MONTH(Last_Assessed__c+1), 1,IF(MOD(Year(Last_Assessed__c+1),4)=0,182,181),2,IF(MOD(Year(Last_Assessed__c+1),4)=0,182,181),3,184,4,183,5,184,6,183,7,184,8, 
IF(MOD(Year(Last_Assessed__c)+1,4)=0,CASE(DAY(Last_Assessed__c),29,184,30,183,31,182,184),CASE(DAY(Last_Assessed__c),30,182,31,183,184)), 
9,IF(MOD(Year(Last_Assessed__c+1)+1,4)=0,182,181),10,IF(MOD(Year(Last_Assessed__c+1)+1,4)=0,183,182),11,IF(MOD(Year(Last_Assessed__c+1)+1,4)=0,182,181),12,IF(MOD(Year(Last_Assessed__c+1)+1,4)=0,183,182),NULL), 

IF(ISPICKVAL(Assessment_Frequency__c, &quot;Annually&quot;), Last_Assessed__c+IF(OR(MONTH(Last_Assessed__c+1)=1,MONTH(Last_Assessed__c+1)=2),IF(MOD(Year(Last_Assessed__c+1),4)=0,366,365),IF(MOD(Year(Last_Assessed__c+1)+1,4)=0,366,365)), 
NULL)))))) 

/* 
1 - 31 
2 - 28/29 
3 - 31 
4 - 30 
5 - 31 
6 - 30 
7 - 31 
8 - 31 
9 - 30 
10 - 31 
11 - 30 
12 - 31 
*/</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Next Assessment Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
