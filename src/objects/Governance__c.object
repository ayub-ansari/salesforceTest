    <validationRules>
        <fullName>Degree_Required_Max_is_100_percent</fullName>
        <active>true</active>
        <description>Degree Required Must be 100% or Less.</description>
        <errorConditionFormula>Degree_Of_Governance_Required__c  &gt;= 100 ||  Degree_Of_Governance_Required__c   &lt; 0</errorConditionFormula>
        <errorDisplayField>Degree_Of_Governance_Required__c</errorDisplayField>
        <errorMessage>Degree Required Must be Between 100% and 0%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>If_Subsidiary_than_Must_have_Parent</fullName>
        <active>true</active>
        <description>If Subsidiary is selected you must have a Parent Governance</description>
        <errorConditionFormula>IF(ISPICKVAL( Parent_or_Subsidiary_Governance__c , &quot;Subsidiary&quot;) &amp;&amp;  ISBLANK(Parent_Governance__c ), TRUE,FALSE)</errorConditionFormula>
        <errorDisplayField>Parent_or_Subsidiary_Governance__c</errorDisplayField>
        <errorMessage>If Subsidiary is selected you must have a Parent Governance</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Less_than_100_Required</fullName>
        <active>true</active>
        <description>If the Degree Required is less than 100% you must provide an explanation.</description>
        <errorConditionFormula>If( Degree_Of_Governance_Required__c  &lt; 1.0 ,  ISBLANK( Explanation_for_under_100__c), False)</errorConditionFormula>
        <errorDisplayField>Explanation_for_under_100__c</errorDisplayField>
        <errorMessage>If the Degree Required is less than 100% you must provide an explanation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Parent_Governance_Cannot_have_Parent</fullName>
        <active>true</active>
        <description>If you select Parent Governance than you can not have a lookup to another Parent Governance</description>
        <errorConditionFormula>IF(ISPICKVAL( Parent_or_Subsidiary_Governance__c ,&quot;Parent&quot;) &amp;&amp; NOT(ISBLANK( Parent_Governance__c )),TRUE,FALSE)</errorConditionFormula>
        <errorDisplayField>Parent_Governance__c</errorDisplayField>
        <errorMessage>If you select Parent Governance than you can not have a lookup to another Parent Governance</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Review_Date_Must_be_After_Enact_Date</fullName>
        <active>true</active>
        <description>Date of Next Review Must come After the Date Enacted.</description>
        <errorConditionFormula>Date_Next_Review__c   &lt;   Date_Governance_Enacted__c</errorConditionFormula>
        <errorDisplayField>Date_Next_Review__c</errorDisplayField>
        <errorMessage>Date of Next Review Must come After the Date Enacted.</errorMessage>
    </validationRules>
</CustomObject>
