    <validationRules>
        <fullName>Current_Must_be_Greater_than_Target</fullName>
        <active>true</active>
        <description>Current values must be greater than or equal to Target values for Risks.</description>
        <errorConditionFormula>IF( OR ( ( $RecordType.Name = &quot;Risk&quot; ), ( $RecordType.Name = &quot;Risk - Binomial&quot; ), ( $RecordType.Name = &quot;Risk - Expected&quot; ), ( $RecordType.Name = &quot;Risk - Normal&quot; ), ( $RecordType.Name = &quot;Risk - Triangular&quot; ) ), (
     Value(Text(Expected_Target_Impact_Number__c))   &gt;   Value(Text(Expected_Residual_Impact_Number__c))   
||   Value(Text(Expected_Target_Likelihood_Number__c))   &gt;   Value(Text(Expected_Residual_Likelihood_Number__c))   
||   Value(Text(Maximum_Target_Impact_Number__c))  &gt;   Value(Text(Maximum_Residual_Impact_Number__c))  
||   Value(Text(Maximum_Target_Likelihood_Number__c))   &gt;    Value(Text(Maximum_Residual_Likelihood_Number__c ))   
||   Value(Text(Minimum_Target_Impact_Number__c  )) &gt;   Value(Text(Minimum_Residual_Impact_Number__c ))  
||   Value(Text(Minimum_Target_Likelihood_Number__c ))  &gt;   Value(Text(Minimum_Residual_Likelihood_Number__c))
),
1=2)</errorConditionFormula>
        <errorMessage>Current values must be greater than or equal to Target values for Risks.</errorMessage>
    </validationRules>
