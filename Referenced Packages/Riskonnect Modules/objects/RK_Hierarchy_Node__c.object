        <errorConditionFormula>if( $Profile.Name &lt;&gt;&quot;RK Data Manager&quot;, if(ischanged(Node_Level__c),True,False),False)</errorConditionFormula>
        <errorDisplayField>Node_Level__c</errorDisplayField>
        <errorMessage>You can not change Node Level</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Can_not_Change_Node_Name</fullName>
        <active>false</active>
        <description>You can not change Node Name</description>
        <errorConditionFormula>if( $Profile.Name &lt;&gt;&quot;RK Data Manager&quot;, if(ischanged(Name),True,False),False)</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>You can not change Node Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Opened_filled_in_if_Date_Closed_is</fullName>
        <active>false</active>
        <description>This Validates that a Location has an Open Date if it has a Closed Date.</description>
        <errorConditionFormula>if( $Profile.Name &lt;&gt;&quot;RK Data Manager&quot;, (IF ( ISBLANK(Date_Location_Opened__c)  &amp;&amp;  NOT( ISBLANK( Date_Location_Closed__c ) ) , true, false)) ,false)</errorConditionFormula>
        <errorDisplayField>Date_Location_Opened__c</errorDisplayField>
        <errorMessage>Must have an Open Date for Locations with Closed Dates.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Relationship_Validation</fullName>
        <active>false</active>
        <description>This is to Validate that the active date does not come before the inactive date.</description>
        <errorConditionFormula>if( $Profile.Name &lt;&gt;&quot;RK Data Manager&quot;, (IF(  Date_Active__c   &gt;   Date_Inactive__c , true, false)) ,false)</errorConditionFormula>
        <errorDisplayField>Date_Inactive__c</errorDisplayField>
        <errorMessage>The Active Date cannot come before the Inactive Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Parent_Required_if_not_Top_Level</fullName>
        <active>false</active>
        <description>Parent level is required if the node is not the top level of the hierarchy.</description>
        <errorConditionFormula>if( $Profile.Name &lt;&gt;&quot;RK Data Manager&quot;, (IF(Node_Level__c &lt;&gt; 1  &amp;&amp;   ISBLANK( Parent_Level_Formula__c )  , true, false)) ,false)</errorConditionFormula>
        <errorMessage>Must provide a parent level value for any node at level 5 or below.</errorMessage>
    </validationRules>
</CustomObject>
